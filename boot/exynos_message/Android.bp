cc_defaults {
	name: "lib_exynos_message_defaults",
	srcs: [
		"exynos_message.cpp"
	],
	cflags: [
		"-Wall",
		"-Werror",
	],
	shared_libs: [
		"liblog",
		"libbase",
		"libutils",
	],
	static_libs: [
		"libfstab",
	],
	export_include_dirs: [
		"include",
	],
}

cc_library_static {
	name: "lib_exynos_message",
	defaults: [
		"lib_exynos_message_defaults",
	],
	relative_install_path: "hw",
	vendor: true,
	recovery_available: true,
}
